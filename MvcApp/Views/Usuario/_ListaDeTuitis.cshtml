@model IEnumerable<Biblioteca.Modelos.Tuiti>

@if (Model.Count() > 0)
{ 
    <ul>
    @foreach (var item in Model)
    {
        <li>
            @Html.DisplayFor(modelItem => item.Texto)
            criado em: @Html.DisplayFor(modelItem => item.DataDeCriacao)
        </li>
    }
    </ul>
}
else
{ 
    <p>Nenhum tuiti postado.</p>
}
